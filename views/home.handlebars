<div class="container">
	<div class="jumbotron mt-2">
		<h1 class="mt-0 mb-5 text-center">{{title}}</h1>
		<div class="row mx-4">
			{{#each data}}
			<div class="col-lg-4 col-md-4 col-sm-12 mb-4 mx-auto">
				<div class="card bg-primary text-dark center">
					<a href="/products/{{this.id}}" class="text-light" target="_blank">
						<img class="card-img-top" src="{{this.pic}}">
						<!-- style="object-fit: cover;" height="100em" -->
						<div class="">
								<h6 class="card-title text-center my-3">See More</h5>
						</div>
					</a>
				</div>
			</div>
			{{/each}}
		</div>
	</div>
</div>
